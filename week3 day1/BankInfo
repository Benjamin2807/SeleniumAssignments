package week3.org.Banking;

public class BankInfo 
{

 	public void savings()
 	{
 		System.out.println(" Savings is 10,000");
 	}
	
 	
 	public void Deposit()
 	{
 		System.out.println("Deposited 50,000 in Hdfc bank");
 		
 	}

 	public void Fixed()
 	{
 		System.out.println("No Fixed Amount");
 	}

}



package week3.org.Banking;

public class AxisBank extends BankInfo{
	
	public void Deposit()
	{
		System.out.println("Deposited 1 Lakh in Axis Bank");
	}
	
	
	public static void main(String args[])
	{
		AxisBank ab = new AxisBank();
		
		ab.Deposit();
		ab.savings();
		ab.Fixed();
		
		BankInfo bi = new BankInfo();
		bi.Deposit();
		bi.savings();
		bi.Fixed();
		
	}
	
	

}
